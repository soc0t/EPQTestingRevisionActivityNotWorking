<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comepqtesting.wix.socot.epqtesting" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2" >
        </activity>
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".AlarmClock"
            android:label="@string/title_activity_alarm_clock" >
        </activity>

        <receiver android:name=".Alarm_Reciever" />

        <service
            android:name=".RingtonePlayingService"
            android:enabled="true" >
        </service>

        <activity
            android:name=".Homework"
            android:label="@string/title_activity_homework" >
        </activity>
        <activity
            android:name=".Calculator"
            android:label="@string/title_activity_calculator" >
        </activity>
        <activity
            android:name=".Timetable"
            android:label="@string/title_activity_timetable" >
        </activity>
        <activity
            android:name=".Revision"
            android:label="@string/title_activity_revision" >
        </activity>
    </application>

</manifest>
